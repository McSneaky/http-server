{
  "name": "@adonisjs/http-server",
  "version": "7.0.0-1",
  "description": "AdonisJS HTTP server with support packed with Routing and Cookies",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types/main.js",
    "./factories": "./build/factories/main.js"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "cross-env NODE_DEBUG=adonisjs:http c8 npm run quick:test",
    "clean": "del-cli build",
    "typecheck": "tsc --noEmit",
    "precompile": "npm run lint && npm run clean",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "build": "npm run compile",
    "prebenchmark": "npm run build",
    "benchmark": "node benchmarks/index.js",
    "release": "np",
    "version": "npm run build",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/http-server",
    "test:watch": "node --watch --loader=ts-node/esm --experimental-import-meta-resolve bin/test.ts",
    "quick:test": "node --loader=ts-node/esm --experimental-import-meta-resolve bin/test.ts"
  },
  "keywords": [
    "http",
    "http-server"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/application": "^8.0.0-2",
    "@adonisjs/encryption": "^5.1.2-4",
    "@adonisjs/eslint-config": "^1.1.9",
    "@adonisjs/events": "^9.0.0-1",
    "@adonisjs/fold": "^9.9.3-11",
    "@adonisjs/logger": "^5.4.2-7",
    "@adonisjs/prettier-config": "^1.1.9",
    "@adonisjs/tsconfig": "^1.1.9",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@fastify/middie": "^8.3.0",
    "@japa/assert": "^2.0.1",
    "@japa/expect-type": "^2.0.0",
    "@japa/runner": "^3.1.0",
    "@swc/core": "^1.3.99",
    "@types/accepts": "^1.3.7",
    "@types/content-disposition": "^0.5.8",
    "@types/cookie": "^0.5.4",
    "@types/destroy": "^1.0.3",
    "@types/encodeurl": "^1.0.2",
    "@types/etag": "^1.8.3",
    "@types/fresh": "^0.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/http-status-codes": "^1.2.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.9.4",
    "@types/on-finished": "^2.3.4",
    "@types/pem": "^1.14.4",
    "@types/proxy-addr": "^2.0.3",
    "@types/qs": "^6.9.10",
    "@types/supertest": "^2.0.16",
    "@types/type-is": "^1.6.6",
    "@types/vary": "^1.1.3",
    "@vinejs/vine": "^1.6.0",
    "autocannon": "^7.12.0",
    "c8": "^8.0.1",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.54.0",
    "fastify": "^4.24.2",
    "fs-extra": "^11.1.1",
    "get-port": "^7.0.0",
    "github-label-sync": "^2.3.1",
    "http-status-codes": "^2.3.0",
    "husky": "^8.0.3",
    "np": "^8.0.4",
    "pem": "^1.14.8",
    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@poppinss/macroable": "^1.0.0",
    "@poppinss/matchit": "^3.1.2",
    "@poppinss/middleware": "^3.2.1",
    "@poppinss/utils": "^6.5.1",
    "@sindresorhus/is": "^6.1.0",
    "accepts": "^1.3.8",
    "content-disposition": "^0.5.4",
    "cookie": "^0.6.0",
    "destroy": "^1.2.0",
    "encodeurl": "^1.0.2",
    "etag": "^1.8.1",
    "fresh": "^0.5.2",
    "mime-types": "^2.1.35",
    "on-finished": "^2.4.1",
    "proxy-addr": "^2.0.7",
    "qs": "^6.11.2",
    "tmp-cache": "^1.1.0",
    "type-is": "^1.6.18",
    "vary": "^1.1.2",
    "youch": "^3.3.3"
  },
  "peerDependencies": {
    "@adonisjs/application": "^8.0.0-2",
    "@adonisjs/encryption": "^5.1.2-4",
    "@adonisjs/events": "^9.0.0-1",
    "@adonisjs/fold": "^9.9.3-11",
    "@adonisjs/logger": "^5.4.2-7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/http-server.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/http-server/issues"
  },
  "homepage": "https://github.com/adonisjs/http-server#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "next",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "factories/**",
      ".yalc/**"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "tsup": {
    "entry": [
      "./index.ts",
      "./src/types/main.ts",
      "./factories/main.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
