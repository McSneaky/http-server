{
  "name": "@adonisjs/http-server",
  "version": "6.8.2-6",
  "description": "AdonisJS HTTP server with support packed with Routing and Cookies",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "src",
    "factories",
    "index.ts",
    "build/src",
    "build/factories",
    "build/index.d.ts",
    "build/index.d.ts.map",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types/main.js",
    "./factories": "./build/factories/main.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "cross-env NODE_DEBUG=adonisjs:http c8 npm run quick:test",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prebenchmark": "npm run build",
    "benchmark": "node --experimental-import-meta-resolve build/benchmarks/index.js",
    "release": "np",
    "version": "npm run build",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/http-server",
    "test:watch": "node --watch --loader=ts-node/esm --experimental-import-meta-resolve bin/test.ts",
    "quick:test": "node --loader=ts-node/esm --experimental-import-meta-resolve bin/test.ts"
  },
  "keywords": [
    "http",
    "http-server"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/application": "^7.1.2-6",
    "@adonisjs/encryption": "^5.1.2-2",
    "@adonisjs/events": "^8.4.9-3",
    "@adonisjs/fold": "^9.9.3-5",
    "@adonisjs/logger": "^5.4.2-3",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@fastify/middie": "^8.3.0",
    "@japa/assert": "^2.0.0-1",
    "@japa/expect-type": "^2.0.0-0",
    "@japa/runner": "^3.0.0-3",
    "@swc/core": "^1.3.67",
    "@types/accepts": "^1.3.5",
    "@types/content-disposition": "^0.5.5",
    "@types/cookie": "^0.5.1",
    "@types/destroy": "^1.0.0",
    "@types/encodeurl": "^1.0.0",
    "@types/etag": "^1.8.1",
    "@types/fresh": "^0.5.0",
    "@types/fs-extra": "^11.0.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^20.3.3",
    "@types/on-finished": "^2.3.1",
    "@types/pem": "^1.9.6",
    "@types/proxy-addr": "^2.0.0",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.12",
    "@types/type-is": "^1.6.3",
    "@types/vary": "^1.1.0",
    "autocannon": "^7.11.0",
    "c8": "^8.0.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^3.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.19.1",
    "fs-extra": "^11.1.1",
    "github-label-sync": "^2.3.1",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.3",
    "np": "^8.0.4",
    "pem": "^1.14.8",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.1",
    "@poppinss/macroable": "^1.0.0-7",
    "@poppinss/matchit": "^3.1.2",
    "@poppinss/middleware": "^3.1.3",
    "@poppinss/utils": "^6.5.0-3",
    "@sindresorhus/is": "^5.3.0",
    "accepts": "^1.3.8",
    "content-disposition": "^0.5.4",
    "cookie": "^0.5.0",
    "destroy": "^1.2.0",
    "encodeurl": "^1.0.2",
    "etag": "^1.8.1",
    "fresh": "^0.5.2",
    "mime-types": "^2.1.35",
    "on-finished": "^2.4.1",
    "proxy-addr": "^2.0.7",
    "qs": "^6.11.2",
    "tmp-cache": "^1.1.0",
    "type-is": "^1.6.18",
    "vary": "^1.1.2",
    "youch": "^3.2.3"
  },
  "peerDependencies": {
    "@adonisjs/application": "^7.1.2-3",
    "@adonisjs/encryption": "^5.1.2-1",
    "@adonisjs/events": "^8.4.9-2",
    "@adonisjs/fold": "^9.9.3-4",
    "@adonisjs/logger": "^5.4.2-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/http-server.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/http-server/issues"
  },
  "homepage": "https://github.com/adonisjs/http-server#readme",
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "backup"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "next",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "factories/**",
      ".yalc/**"
    ]
  }
}
